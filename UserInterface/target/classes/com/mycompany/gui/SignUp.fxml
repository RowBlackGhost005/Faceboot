<AnchorPane id="AnchorPane" prefHeight="540.0" prefWidth="960.0" styleClass="mainFxmlClass" stylesheets="@../../../css/signUp.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.gui.SignUpController">
   <children>
      <TextField fx:id="txtName" layoutX="235.0" layoutY="127.0" promptText="Name" />
      <TextField fx:id="txtEmail" layoutX="234.0" layoutY="188.0" promptText="Email" />
