      <Label layoutX="353.0" layoutY="32.0" styleClass="title" text="Create an account" />
      <Button fx:id="btnSignUp" layoutX="236.0" layoutY="445.0" mnemonicParsing="false" onMouseClicked="#clickBtnSignUp" styleClass="register" text="Sign Up" />
